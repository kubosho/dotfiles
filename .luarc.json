{
  "$schema": "https://raw.githubusercontent.com/LuaLS/vscode-lua/master/setting/schema.json",
  "diagnostics": {
    "globals": ["utf8", "vim"]
  },
  "runtime": {
    "version": "LuaJIT"
  },
  "workspace": {
    "ignoreDir": ["/.vscode", "/3rd", "/libs/", "/locale/", "/meta", "/node_modules"],
    "library": ["lua", "$VIMRUNTIME", "${3rd}/luv/library"],
    "maxPreload": 1600,
    "preloadFileSize": 1000
  }
}
