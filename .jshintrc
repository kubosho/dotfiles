{
  /* 制限 */
  // ループ,条件ブロックを括弧で囲む事を強制
  "curly": true,
  // ==, != を禁止
  "eqeqeq": true,
  // 括弧で囲まれていない関数の即時実行を禁止
  "immed": true,
  // 変数を定義する前に使うのを禁止
  "latedef": true,
  // 一行の最大文字数を制限
  "maxlen": 80,
  // コンストラクタ関数の一文字目を大文字にすることを強制
  "newcap": true,
  // 明示的に宣言されていない変数の使用を禁止
  "undef": true,
  // 変数が利用されていない場合に警告
  "unused": true,
  // 行末の空白を禁止
  "trailing": true,

  /* 環境指定 */
  // ブラウザーのグローバルオブジェクトを定義済みにする
  "browser": true,
  // consoleやalertを定義済みにする
  "devel": true,
  // jQueryを定義済みにする
  "jquery": true,
  // nodeのグローバルオブジェクトを定義済みにする
  "node": true,
  // WebWorkerのグローバルオブジェクトを定義済みにする
  "worker": true
}

