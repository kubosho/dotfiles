<script>
(function (win, doc, ns) {
/* -------------------------------------
    IMPORT
----------------------------------------- */
var Stage  = ns.Stage,
    Actor  = ns.Actor,
    Actors = ns.Actors,
    Line   = ns.Line,
    Dot    = ns.Dot,

    //for Graph
    Graph       = ns.Graph,
    GraphData   = ns.GraphData,
    GraphMemory = ns.GraphMemory,
    GraphSpan   = ns.GraphSpan;


//An entry point.
function main() {
    window.console.log('Start app');

    var cont = document.getElementById('rateGraph');

    //data.
    var data = new GraphData({
        data: [ 220, 880, 660, 1500, 800, 920, 1000, 800, 900, 950 ],
        color: '#C00'
    });

    //Graph set up.
    var graph = new Graph(cont, {
        data: [data],
        memory: {
            min: 0,
            max: 2200,
            div: 10
        },
        span: ['4/4', '4/11', '4/18', '4/25', '5/1', '5/8', '5/15', '5/22', '5/29', '6/5']
    });

    graph.on('drawend', function (e) {
        //do something if you want when `graph` ended.
    }, false);

    window.setTimeout(function () {
        graph.draw();
    }, 500);

    document.addEventListener('click', function (e) {
        graph.select();
        return false;
    }, false);

    window.graph = graph;
}

//Start.
window.addEventListener('DOMContentLoaded', main, false);
}(window, document, app));
</script>
